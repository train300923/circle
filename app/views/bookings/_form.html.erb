<div class="container" id="form-format">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <%= simple_form_for [@activity, @booking] do |f| %>
        <%= f.input :start_date %>
        <%= f.input :end_date %>
        <%= f.button :submit, "Submit", class: "submit-button" %>
      <% end %>
    </div>
  </div>
</div>
